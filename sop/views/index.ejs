<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/stylesheets/stylesb.css" rel="stylesheet">
    <link href="/stylesheets/bootstrap.min.css" rel="stylesheet">
    <link href="./fontawesome/css/all.css" rel="stylesheet">
    <link href="./stylesheets/styledd.css" rel="stylesheet">
    <link href="./stylesheets/style4.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa"
        crossorigin="anonymous"></script>
    <title>Dashboard</title>
    <script src="/jquery-3.6.1.min.js"></script>
</head>

<body>
    <div class="main-container d-flex">
        <%- include('./partials/sidebar') %>

            <div class="hero">
                <nav class="navbar">
                    <div class="container-fluid">
                        <div class="d-flex d-md-none d-block">
                            <button class="btn px-1 py-0 open-btn"><i class="fa-solid fa-bars-staggered"></i></button>
                            <a href="#" class="navbar-brand fs-4"><span
                                    class="bg-dark rounded px-2 py-20 text-white">PoS</span></a>
                        </div>
                        <form class="d-flex" role="search">
                            <input class="form-control me-2 search" type="search" placeholder="Search for..."
                                aria-label="Search">
                            <button class="btn btn-outline-primary" type="submit"><i
                                    class="fa-solid fa-magnifying-glass"></i></button>
                        </form>
                        <i class="fa-regular fa-bell bell"></i>
                        <div class="solid"></div>
                        <a class="name" onclick="menuToggle()">
                            <%= users.name %>
                                Agung harwin n
                        </a>
                        <div class="action">
                            <div class="profile" onclick="menuToggle();">
                                <img src="/images/112538956.jpeg" alt="">
                            </div>

                            <div class="menu">
                                <h3>
                                    <%= users.name %>
                                        agung harwin n
                                        <div>
                                            Operational Team
                                        </div>
                                </h3>
                                <ul class="ul">
                                    <li>
                                        <i class="fa-solid fa-user-large"></i>
                                        <a class="a" href="#"> Profile</a>
                                    </li>
                                    <li>
                                        <i class="fa-solid fa-gears"></i>
                                        <a href="#">Settings</a>
                                    </li>
                                    <li>
                                        <i class="fa-solid fa-list"></i>
                                        <a href="#"> Activity Log</a>
                                    </li>
                                    <hr>
                                    <li>
                                        <i class="fa-solid fa-right-from-bracket"></i>
                                        <a href="#" onclick="$('#modal-logout').modal('show')"> Logout</a>
                                    </li>

                                </ul>
                            </div>
                        </div>
                        <div class="modal fade" id="modal-logout" tabindex="-1" role="dialog"
                            aria-labelledby="exampleModalLabel" aria-hidden="true">
                            <div class="modal-dialog" role="document">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h5 class="modal-title">Do yo want to leave?</h5>
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"
                                            onclick="$('#modal-logout').modal('hide')">
                                            <span aria-hidden="true">&times;</span>
                                        </button>
                                    </div>
                                    <div class="modal-body">
                                        <p>Select Logout to end your session?</p>
                                    </div>
                                    <div class="modal-footer">
                                        <button id="btn-logout" type="button" class="btn btn-primary"
                                            onclick="$('#logout').modal('hide')">Logout</button>
                                        <button type="button" class="btn btn-secondary" data-dismiss="modal"
                                            onclick="$('#modal-logout').modal('hide')">Cancel</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
              

                    <div class="card">
                        <div>
                            <div class="card-header">
                                <button type="button" class="btn btn-primary add " title="Add" onclick="addData()"><i
                                    class="fa-regular fa-square-plus" title="Add"></i>
                                  ADD
                                </button>
                            </div>
                             
                            
                                <input class="form-control me-2 fsearch" type="search" placeholder="Search for..."
                                    aria-label="Search">
     
                            <table class="table table-striped ">
                                <thead class="border">
                                    <tr>
                                        
                                        <th>
                                            <a>User ID
                                            </a>
                                        </th>

                                        <th>
                                            <a>Email
                                            </a>
                                        </th>

                                        <th> <a>Name
                                            </a>
                                        </th>
                                        <th>
                                            <a>Role
                                            </a>
                                        </th>

                                        <th>Actions </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% users.forEach((item, index)=> { %>
                                        <tr>
                                            <div>
                                               
                                                <td>
                                                    <%= item.userid %>
                                                </td>

                                                <td>
                                                    <%= item.email %>
                                                </td>
                                                <td>
                                                    <%= item.name %>
                                                </td>
                                                <td>
                                                    <%= item.role %>
                                                </td>
                                                <td>
                                                    <div class="d-grid gap-2 d-md-block">
                                                        <a class="btn btn-success" href="/edit/<%= item.id %>">Edit</a>
                                                        <a class="btn btn-danger" href="/delete/<%= item.id %>"
                                                            href="delete_methode_link"
                                                            onclick="return confirm('Are you sure you want to delete?');">Delete
                                                        </a>
                                                    </div>
                                                </td>
                                            </div>
                                        </tr>
                                        <% }) %>
                                </tbody>
                            </table>
                        </div>

                </nav>
            </div>
    </div>

    </div>

    </div>
    <script>

        $('#modal-logout').click(function () {
            $('#logout').show()
        })
        $('#logout').click(function () {
            $('/logout').show()
        })



        function menuToggle() {
            const toggleMenu = document.querySelector('.menu');
            toggleMenu.classList.toggle('active')
        }
    </script>

    <script>
        $(".sidebar ul li").on('click', function () {
            $(".sidebar ul li.active").removeClass('active');
            $(this).addClass('active');
        })
        $('.open-btn').on('click', function () {
            $('.sidebar').addClass('active');
        })
        $('.close-btn').on('click', function () {
            $('.sidebar').removeClass('active');
        })

    </script>
    <script>
        function menuToggle() {
            const toggleMenu = document.querySelector('.menu');
            toggleMenu.classList.toggle('active')
        }
    </script>

    <%- include('./partials/footer') %>
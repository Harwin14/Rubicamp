<%- include('partials/header') %>
    <div>
        <h1>Add Data</h1>
        <form action=" " method="post">
            <div class="row mb-3">
                <div class="col-sm-2 col-form-label">
                    <label for="string">String</label>
                </div>
                <div class="col-sm-10">
                    <input class="form-control" type="text" name="string" id="string" placeholder=" String " required
                        autofocus>
                </div>
            </div>

            <div class="row mb-3">
                <div class="col-sm-2 col-form-label">
                    <label for="integer">Integer</label>
                </div>
                <div class="col-sm-10">
                    <input class="form-control" type="number" name="integer" id="integer" placeholder=" Integer "
                        required>
                </div>
            </div>
            <div class="row mb-3">
                <div class="col-sm-2 col-form-label">
                    <label for="float">Float</label>
                </div>
                <div class="col-sm-10">
                    <input class="form-control" type="number" step="0.05" name="float" id="float" placeholder="Float"
                        required>
                </div>
            </div>

            <div class="row mb-3">
                <div class="col-sm-2 col-form-label">
                    <label for="date">Date</label>
                </div>
                <div class="col-sm-10">
                    <input class="form-control" type="date" name="date" id="date">
                </div>
            </div>
            <div class="row mb-3">
                <div class="col-sm-2 col-form-label">
                    <div class="col-sm-10">
                        <label for="boolean" placeholder="Choose Boolean">Boolean</label>
                    </div>
                </div>
                <div class="col-sm-10">
                    <select class="form-control" name="boolean" id="boolean">
                        <option disabled selected> Choose the boolean ... </option>
                        <option value="true">true</option>
                        <option value="false">false</option>
                    </select>
                </div>
            </div>
            <div class="d-grid gap-2 d-md-block">
                    <button class="btn btn-primary" type="submit">Save</a>
                <a class="btn btn-warning" href="/">Back</a>
            </div>
            <div>

            </div>

        </form>
    </div>
